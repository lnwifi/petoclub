{
  "expo": {
    "name": "PetoClub",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "config": {
        "extra": {
          "metroConfig": {
            "resolver": {
              "extraNodeModules": {
                "stream": "stream-browserify",
                "crypto": "crypto-browserify",
                "buffer": "buffer",
                "process": "process/browser"
              }
            }
          }
        }
      }
    },
    "plugins": [
      "expo-router",
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2c4e536b-1b0e-48ea-86b5-7c92037a5a4f"
      }
    },
    "android": {
      "package": "com.petoclub.boltexponativewind"
    }
  }
}
